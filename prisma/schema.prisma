generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model fecfechas {
  fecid      Int       @id @default(autoincrement())
  fecnombre  String    @db.VarChar(255)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
  feccurso   Boolean   @default(false)
}

model grugrupos {
  gruid      Int       @id @default(autoincrement())
  grugrupo   String    @db.VarChar(255)
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
}

model parpartidos {
  parid           Int       @id @default(autoincrement())
  parlocal        Int
  parlvisitante   Int
  parfinalizado   Boolean   @default(false)
  pargollocal     Int
  pargolvisitante Int
  parfecid        Int
  pargrupo        Int
  parganador      Int?
  created_at      DateTime? @default(now()) @db.Timestamp(0)
  updated_at      DateTime? @default(now()) @db.Timestamp(0)
}

model preprediciones {
  preid           Int       @id @default(autoincrement())
  preusuid        Int
  preparid        Int
  prefinalizado   Boolean   @default(false)
  pregollocal     Int?
  pregolvisitante Int?
  prepuntos       Int?
  preganador      Int?
  created_at      DateTime? @default(now()) @db.Timestamp(0)
  updated_at      DateTime? @default(now()) @db.Timestamp(0)
}

model selseleccion {
  selid          Int       @id @default(autoincrement())
  selnombre      String    @db.VarChar(255)
  selabreviacion String    @db.VarChar(255)
  selescudo      String?   @db.VarChar(255)
  created_at     DateTime? @default(now()) @db.Timestamp(0)
  updated_at     DateTime  @default(now()) @db.Timestamp(0)
}

model usuusuarios {
  usuid          Int       @id @default(autoincrement())
  usuusuario     String    @db.VarChar(255)
  usucontrasenia String    @db.VarChar(255)
  usutoken       String    @db.VarChar(255)
  usunombre      String    @db.VarChar(255)
  created_at     DateTime? @default(now()) @db.Timestamp(0)
  updated_at     DateTime? @default(now()) @db.Timestamp(0)
}
